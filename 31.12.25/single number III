class Solution(object):
    def singleNumber(self, nums):
        """
        :type nums: List[int]
        :rtype: List[int]
        """
        nums.sort()

        stk=[]
        stk.append(nums[0])

        for i in range(1, len(nums)):
            if(stk and nums[i] == stk[-1]):
                stk.pop()
            elif(not stk or nums[i] != stk[-1]):
                stk.append(nums[i])

        return stk
